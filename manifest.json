{
    "manifest_version": 3,
    "name": "Tab Closeout Timer",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    }, 
    "host_permissions": ["https://developer.chrome.com/*"],
    "permissions": ["tabs", "tabGroups", "activeTab", "storage"],
    "background": {
    "service_worker": "background.js"
  }
}